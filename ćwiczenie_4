#include <iostream>
#include <conio.h>
#include <windows.h>
#include <cstdlib>
void gotoxy(int x, int y );
using namespace std;
int main()
{
  gotoxy(15,5);
   cout << "//";
   gotoxy(16,5);
   cout << "\\";
   gotoxy(17,6);
   cout << "\\";
   gotoxy(14,6);
   cout << "//";
   
}

void gotoxy( int x, int y ) 
{
 COORD coord;
 coord.X = x; 
 coord.Y = y; 
 
 SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), coord );
 }
